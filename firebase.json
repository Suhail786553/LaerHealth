{
  "storage": {
    "rules": "storage.rules"
  },
  "hosting": {
    "public": "public",
    "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          }
        ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  }
}
